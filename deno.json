{
  "name": "@catppuccin/palette",
  "version": "1.7.1",
  "exports": "./mod.ts",
  "importMap": "./import_map.json",
  "exclude": [
    "dist"
  ],
  "fmt": {
    "exclude": [
      "*.md"
    ]
  },
  "tasks": {
    "build:npm": "deno run -A ./scripts/build_npm.ts",
    "build:palettes": "deno run -A ./scripts/build_palettes.ts",
    "generate": "deno run -A ./scripts/gen_palette.ts",
    "build:vscode": "deno run -A ./scripts/gen_vscode_overrides.ts",
    "build:ghostty": "deno run -A ./scripts/gen_ghostty_themes.ts",
    "build": "deno task build:npm & deno task build:palettes & deno task build:ghostty",
    "demo": "deno run --allow-net --allow-read server.ts"
  }
}
